{
  "name": "nestjs-mixpanel",
  "version": "1.0.1",
  "description": "NestJS Mixpanel integration module",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:memory": "node --expose-gc ./node_modules/vitest/vitest.mjs run src/__tests__/e2e/mixpanel-memory.e2e.test.ts",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "nestjs",
    "mixpanel",
    "analytics",
    "tracking"
  ],
  "author": "ESnark<settimeout1000@gmail.com>",
  "license": "ISC",
  "packageManager": "pnpm@10.8.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "nestjs-cls": "^6.0.1"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "mixpanel": "^0.18.1",
    "nestjs-cls": "^6.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/testing": "^11.1.3",
    "@swc/core": "^1.12.11",
    "@types/mixpanel": "^2.14.9",
    "@types/node": "^24.0.12",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "supertest": "^7.1.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}